<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1.0.1-AuthorDoil" author="aelshin">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT
                  COUNT(AUTHORID)
                FROM
                  LIB_AUTHORS
                WHERE 1=1
                  AND LASTNAME = 'Дойл'
                  AND FIRSTNAME = 'Артур'
                  AND MIDDLENAME = 'Конан'
            </sqlCheck>
        </preConditions>

        <insert tableName="LIB_AUTHORS">
            <column name="AUTHORID" valueComputed="nextval('LIB_AUTHORS_SEQ')"/>
            <column name="LASTNAME">Дойл</column>
            <column name="FIRSTNAME">Артур</column>
            <column name="MIDDLENAME">Конан</column>
        </insert>

    </changeSet>

