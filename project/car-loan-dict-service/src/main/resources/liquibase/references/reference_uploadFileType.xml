<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1.0.1-AutoLoanReferenceBodyType" author="aelshin">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT
                  COUNT(REFERENCEID)
                FROM
                  REF_REFERENCE
                WHERE
                  REFSYSNAME = 'uploadFileType'
                  AND REFGROUPID IN (
                    SELECT
                      REFGROUPID
                    FROM
                      REF_REFGROUP
                    WHERE
                      PARENTGROUPID = 0
                      AND GROUPSYSNAME = 'AutoLoan'
                )
            </sqlCheck>
        </preConditions>

        <insert tableName="REF_REFERENCE">
            <column name="REFERENCEID" valueComputed="nextval('REF_REFERENCE_SEQ')"/>
            <column name="REFGROUPID" valueComputed="(SELECT REFGROUPID FROM REF_REFGROUP WHERE PARENTGROUPID = 0 AND GROUPSYSNAME = 'AutoLoan')"/>
            <column name="REFERENCENAME">Типы загружаемых файлов</column>
            <column name="REFSYSNAME">uploadFileType</column>
            <column name="DESCRIPTION"></column>
        </insert>

    </changeSet>

